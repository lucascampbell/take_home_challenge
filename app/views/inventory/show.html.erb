<div class='row'>
  <table class='table tabled-striped'>
    <tr>
      <th>id</th>
      <th>color</th>
      <th>size</th>
      <th>status</th>
      <th>style</th>
    </tr>
    <%@items.each do |item|%>
    <tr>
      <td><%=item.id%></td>
      <td><%=item.color%></td>
      <td><%=item.size%></td>
      <td><%=item.status%></td>
      <td><%=item.style.name%></td>
    </tr>
    <%end%>
  </table>
</div>